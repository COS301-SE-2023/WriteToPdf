{
  "compileOnSave": false,
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./",
    "resolveJsonModule": true,
    "paths": {
    "path": ["node_modules/path-browserify"],
    "util": ["node_modules/util/"],
    "assert": ["node_modules/assert"],
    "stream": ["node_modules/stream-browserify"],
    "crypto": ["node_modules/crypto-browserify"],
    "url": ["node_modules/url/"],
    "querystring": ["node_modules/querystring-es3"],
    "http": ["node_modules/stream-http"],
    "https": ["node_modules/stream-http"],
    "constants": ["node_modules/constants-browserify/"],
    "os": ["node_modules/os-browserify/"],
    "vm": ["node_modules/vm-browserify/"],
    "zlib": ["node_modules/browserify-zlib/"],
    "buffer": ["node_modules/buffer/"],
    "fs": ["node_modules/browserify-fs"],
    "net": ["node_modules/net-browserify"],
    "tls": ["node_modules/browserify-tls"],
    "jsdom": ["node_modules/jsdom"],
    "child_process": ["node_modules/child_process"]
  },
    "outDir": "./dist/out-tsc",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "ES2022",
    "module": "ES2022",
    "useDefineForClassFields": false,
    "lib": [
      "ES2022",
      "dom"
    ]
  },
  "angularCompilerOptions": {
    "enableI18nLegacyMessageIdFormat": false,
    "strictInjectionParameters": true,
    "strictInputAccessModifiers": true,
    "strictTemplates": true
  }
}
